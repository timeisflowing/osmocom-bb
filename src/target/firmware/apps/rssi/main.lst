 846:apps/rssi/main.c **** 			toggle_up_down();
 847:apps/rssi/main.c **** 		else if (mode == MODE_ARFCN)
 848:apps/rssi/main.c **** 			enter_arfcn(key_code);
 849:apps/rssi/main.c **** 		else if (mode == MODE_SYNC && cursor < 0)
