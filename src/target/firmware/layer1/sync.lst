  84:layer1/sync.c **** void l1s_time_dump(const struct gsm_time *time)
  85:layer1/sync.c **** {
  86:layer1/sync.c **** 	printf("fn=%lu(%u/%2u/%2u)", time->fn, time->t1, time->t2, time->t3);
  87:layer1/sync.c **** }
