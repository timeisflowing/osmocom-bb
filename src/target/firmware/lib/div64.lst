 158 00b8 010854E3 		cmp	r4, #(1 << 16)
 159 00bc 00C0A0E3 		mov	ip, #0
 160 00c0 2228A021 		movhs	yl, yl, lsr #16
 161 00c4 10C0A023 		movhs	ip, #16
